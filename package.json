		"react-tagsinput": "3.19.0",
		"redux": "4.0.1",
		"redux-thunk": "2.3.0",
		"refx": "3.1.1",
		"resize-observer-polyfill": "1.5.1",
		"sass-loader": "7.1.0",
		"style-loader": "0.23.1",
		"swiper": "4.5.0",
		"uglify-save-license": "0.4.1",
