    "eslint-plugin-react": "7.6.1",
    "eslint-plugin-wpcalypso": "4.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "fancy-log": "^1.3.2",
    "file-loader": "^1.1.5",
    "focus-trap": "^2.3.1",
    "glotpress-js": "0.0.6",
    "grunt-cli": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-babel": "7.0.1",
    "gulp-banner": "^0.1.3",
    "gulp-browserstack": "^1.0.0",
