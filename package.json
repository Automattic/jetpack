    "react-router-redux": "^4.0.8",
    "react-tagsinput": "^3.13.1",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.20",
