    "jshint": "2.9.1",
    "jshint-stylish": "~2.1.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.9.0",
    "node-sass": "^3.7.0",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "0.14.3",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "react-tagsinput": "^3.13.1",
    "react-tap-event-plugin": "0.2.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "sass-loader": "3.1.2",
