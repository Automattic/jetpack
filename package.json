    "glob": "^7.1.1",
    "grunt": "^0.4.2",
    "grunt-wp-i18n": "~0.4.6",
    "mocha": "^2.4.5",
    "mockery": "^1.4.1",
    "nock": "^7.2.2",
    "qunitjs": "~1.20.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "sinon-qunit": "~2.0.0",
    "webpack-dev-server": "1.14.0"
  },
  "engines": {
    "node": ">=6.10.2"
  }
}
