    "url-loader": "^0.6.2",
    "webpack": "3.11.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-register": "^6.8.0",
    "chai": "^4.1.2",
    "commander": "2.14.1",
