    "del": "^3.0.0",
    "es6-promise": "4.2.4",
    "eslint": "4.18.0",
    "eslint-config-wpcalypso": "2.0.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
