{
	"problemMatcher": [
		{
			"owner": "phpcs",
			"severity": "error",
			"pattern": [
				{
					"regexp": "^<file name=\"(.*)\">$",
					"file": 1
				},
				{
					"regexp": "<error line=\"(\\d*)\" column=\"(\\d+)\" severity=\"(error|warning)\" message=\"(.*)\" source=\"(.*)\"\\/>$",
					"line": 1,
					"column": 2,
					"severity": 3,
					"message": 4,
					"code": 5,
					"loop": true
				}
			]
		}
	]
}
