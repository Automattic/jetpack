{
	"problemMatcher": [
		{
			"owner": "phpcs",
			"severity": "error",
			"pattern": [
				{
					"regexp": "^(.+?):(\\d+):(\\d+): ((?i)error|warning) - (.* \\(([^)]+)\\))$",
					"file": 1,
					"line": 2,
					"column": 3,
					"severity": 4,
					"message": 5,
					"code": 6
				}
			]
		}
	]
}
