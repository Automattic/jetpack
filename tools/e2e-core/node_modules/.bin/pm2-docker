#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/adrianmoldovan/repo/github.com/automattic/jetpack/node_modules/.pnpm/pm2@5.1.0/node_modules/pm2/bin/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/node_modules/.pnpm/pm2@5.1.0/node_modules/pm2/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/node_modules/.pnpm/pm2@5.1.0/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/node_modules/.pnpm/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/node_modules:/Users/adrianmoldovan/repo/github.com/node_modules:/Users/adrianmoldovan/repo/node_modules:/Users/adrianmoldovan/node_modules:/Users/node_modules:/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/projects/js-packages/e2e-core/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/projects/js-packages/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/projects/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/node_modules/.pnpm/pm2@5.1.0/node_modules/pm2/bin/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/node_modules/.pnpm/pm2@5.1.0/node_modules/pm2/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/node_modules/.pnpm/pm2@5.1.0/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/node_modules/.pnpm/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/node_modules:/Users/adrianmoldovan/repo/github.com/node_modules:/Users/adrianmoldovan/repo/node_modules:/Users/adrianmoldovan/node_modules:/Users/node_modules:/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/projects/js-packages/e2e-core/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/projects/js-packages/node_modules:/Users/adrianmoldovan/repo/github.com/automattic/jetpack/projects/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node_modules/.pnpm/pm2@5.1.0/node_modules/pm2/bin/pm2-docker" "$@"
else
  exec node  "$basedir/../../../../../node_modules/.pnpm/pm2@5.1.0/node_modules/pm2/bin/pm2-docker" "$@"
fi
