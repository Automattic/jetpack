{
	"name": "jetpack-js-tools",
	"private": true,
	"description": "Avoid cluttering the monorepo root with random JS script deps by putting them in a workspace. This is intended for simple scripts and third-party scripts that we will only run directly or from the monorepo root.",
	"bin": {
		"eslint-changed": "eslint-changed",
		"prettier": "prettier",
		"semver": "semver",
		"sort-package-json": "sort-package-json",
		"validate-es": "validate-es"
	},
	"devDependencies": {
		"@automattic/eslint-changed": "workspace:*",
		"@automattic/eslint-config-target-es": "workspace:*",
		"@babel/core": "7.17.10",
		"@babel/eslint-parser": "7.17.0",
		"@octokit/auth-token": "2.5.0",
		"@octokit/rest": "18.12.0",
		"@rushstack/eslint-patch": "1.1.1",
		"@sveltejs/eslint-config": "github:sveltejs/eslint-config",
		"@typescript-eslint/eslint-plugin": "5.23.0",
		"@typescript-eslint/parser": "5.23.0",
		"@wordpress/eslint-plugin": "12.2.0",
		"chalk": "4.1.2",
		"debug": "4.3.3",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-wpcalypso": "6.1.0",
		"eslint-plugin-es": "4.1.0",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-inclusive-language": "2.2.0",
		"eslint-plugin-jest": "26.2.2",
		"eslint-plugin-jsdoc": "37.9.7",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-lodash": "7.4.0",
		"eslint-plugin-playwright": "0.9.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.29.4",
		"eslint-plugin-react-hooks": "4.5.0",
		"eslint-plugin-svelte3": "3.4.1",
		"eslint-plugin-wpcalypso": "5.0.0",
		"glob": "7.1.6",
		"ignore": "5.1.8",
		"inquirer": "7.3.3",
		"jest": "28.1.0",
		"libsodium-wrappers": "0.7.10",
		"parse-diff": "0.8.1",
		"prettier": "npm:wp-prettier@2.2.1-beta-1",
		"prettier-plugin-svelte": "2.4.0",
		"semver": "7.3.5",
		"sort-package-json": "1.50.0",
		"svelte": "3.42.4",
		"typescript": "4.6.4",
		"yaml": "1.10.2"
	},
	"engines": {
		"node": "^14.18.3 || ^16.13.2",
		"pnpm": "^6.32.3",
		"yarn": "use pnpm instead - see docs/yarn-upgrade.md"
	}
}
