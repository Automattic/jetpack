#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/javier/projects/Jetpack/node_modules/.pnpm/mocha@8.4.0/node_modules/mocha/bin/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/mocha@8.4.0/node_modules/mocha/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/mocha@8.4.0/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/node_modules:/Users/javier/projects/Jetpack/node_modules:/Users/javier/projects/node_modules:/Users/javier/node_modules:/Users/node_modules:/node_modules:/Users/javier/projects/Jetpack/tools/js-test-runner/node_modules:/Users/javier/projects/Jetpack/tools/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/javier/projects/Jetpack/node_modules/.pnpm/mocha@8.4.0/node_modules/mocha/bin/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/mocha@8.4.0/node_modules/mocha/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/mocha@8.4.0/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/node_modules:/Users/javier/projects/Jetpack/node_modules:/Users/javier/projects/node_modules:/Users/javier/node_modules:/Users/node_modules:/node_modules:/Users/javier/projects/Jetpack/tools/js-test-runner/node_modules:/Users/javier/projects/Jetpack/tools/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/mocha@8.4.0/node_modules/mocha/bin/_mocha" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/mocha@8.4.0/node_modules/mocha/bin/_mocha" "$@"
fi
