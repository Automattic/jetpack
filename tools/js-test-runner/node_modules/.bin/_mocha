#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dustyreagan/GitHub/jetpack-atomic/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/dustyreagan/GitHub/jetpack-atomic/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/mocha@10.0.0/node_modules/mocha/bin/_mocha" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/mocha@10.0.0/node_modules/mocha/bin/_mocha" "$@"
fi
