{
	"name": "jetpack-eslint",
	"private": true,
	"description": "Avoid cluttering the monorepo root with eslint deps by putting eslint in a workspace.",
	"bin": {
		"eslint": "eslint",
		"eslint-changed": "eslint-changed"
	},
	"devDependencies": {
		"@automattic/eslint-changed": "workspace:1.0.1-alpha",
		"@babel/core": "7.14.6",
		"@babel/eslint-parser": "7.14.5",
		"@wordpress/eslint-plugin": "7.4.0",
		"eslint": "7.25.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-config-wpcalypso": "6.1.0",
		"eslint-loader": "4.0.2",
		"eslint-plugin-es5": "1.5.0",
		"eslint-plugin-inclusive-language": "1.2.1",
		"eslint-plugin-jest": "24.3.6",
		"eslint-plugin-jsdoc": "32.3.3",
		"eslint-plugin-jsx-a11y": "6.4.1",
		"eslint-plugin-lodash": "7.2.0",
		"eslint-plugin-prettier": "3.4.0",
		"eslint-plugin-react": "7.23.2",
		"eslint-plugin-wpcalypso": "5.0.0"
	},
	"engines": {
		"node": "^14.16.1",
		"pnpm": "^6.5.0",
		"yarn": "use pnpm instead - see docs/yarn-upgrade.md"
	}
}
