#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/andrewserong/dev/a8c-projects/jetpack/projects/js-packages/eslint-changed/bin/node_modules:/Users/andrewserong/dev/a8c-projects/jetpack/projects/js-packages/eslint-changed/node_modules:/Users/andrewserong/dev/a8c-projects/jetpack/projects/js-packages/node_modules:/Users/andrewserong/dev/a8c-projects/jetpack/projects/node_modules:/Users/andrewserong/dev/a8c-projects/jetpack/node_modules:/Users/andrewserong/dev/a8c-projects/node_modules:/Users/andrewserong/dev/node_modules:/Users/andrewserong/node_modules:/Users/node_modules:/node_modules:/Users/andrewserong/dev/a8c-projects/jetpack/tools/eslint/node_modules:/Users/andrewserong/dev/a8c-projects/jetpack/tools/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/andrewserong/dev/a8c-projects/jetpack/projects/js-packages/eslint-changed/bin/node_modules:/Users/andrewserong/dev/a8c-projects/jetpack/projects/js-packages/eslint-changed/node_modules:/Users/andrewserong/dev/a8c-projects/jetpack/projects/js-packages/node_modules:/Users/andrewserong/dev/a8c-projects/jetpack/projects/node_modules:/Users/andrewserong/dev/a8c-projects/jetpack/node_modules:/Users/andrewserong/dev/a8c-projects/node_modules:/Users/andrewserong/dev/node_modules:/Users/andrewserong/node_modules:/Users/node_modules:/node_modules:/Users/andrewserong/dev/a8c-projects/jetpack/tools/eslint/node_modules:/Users/andrewserong/dev/a8c-projects/jetpack/tools/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../projects/js-packages/eslint-changed/bin/eslint-changed.js" "$@"
else
  exec node  "$basedir/../../../../projects/js-packages/eslint-changed/bin/eslint-changed.js" "$@"
fi
