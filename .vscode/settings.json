{ 
	"intelephense.files.exclude": [
	"**/.git/**",
	"**/.svn/**",
	"**/.hg/**",
	"**/CVS/**",
	"**/.DS_Store/**",
	"**/node_modules/**",
	"**/bower_components/**",
	"**/vendor/**/{Tests,tests}/**",
	"**/.history/**",
	"**/vendor/**/vendor/**",
	"**/packages/**/vendor/**",
	"**/projects/**/vendor/**",
	"**/plugins/**/vendor/**",
	"**/packages/**/wordpress/**",
	"**/projects/**/wordpress/**",
	"**/plugins/**/wordpress/**",
	"**/.cache/**",
	"**/jetpack_vendor/**/jetpack_vendor/**",
	"**/jetpack_vendor/**"
	],
	"phpcs.enable": true,
	"phpcs.executablePath": "vendor/bin/phpcs",
	"phpcs.ignorePatterns": [
		"**/.git/**",
		"**/.svn/**",
		"**/.hg/**",
		"**/.cache/**",
		"**/jetpack_vendor/**",
		"**/vendor/**",
	],
	"prettier.prettierPath": "node_modules/.pnpm/wp-prettier@2.8.5/node_modules/wp-prettier/index.js"
}