#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/node_modules/.pnpm/sass@1.43.3/node_modules/sass/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/node_modules/.pnpm/sass@1.43.3/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/node_modules/.pnpm/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/node_modules:/Users/kev/Documents/Docs/Workspace/teams/node_modules:/Users/kev/Documents/Docs/Workspace/node_modules:/Users/kev/Documents/Docs/node_modules:/Users/kev/Documents/node_modules:/Users/kev/node_modules:/Users/node_modules:/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/projects/plugins/reach/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/projects/plugins/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/projects/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/node_modules/.pnpm/sass@1.43.3/node_modules/sass/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/node_modules/.pnpm/sass@1.43.3/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/node_modules/.pnpm/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/node_modules:/Users/kev/Documents/Docs/Workspace/teams/node_modules:/Users/kev/Documents/Docs/Workspace/node_modules:/Users/kev/Documents/Docs/node_modules:/Users/kev/Documents/node_modules:/Users/kev/node_modules:/Users/node_modules:/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/projects/plugins/reach/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/projects/plugins/node_modules:/Users/kev/Documents/Docs/Workspace/teams/a8c/projects/jetpack/projects/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node_modules/.pnpm/sass@1.43.3/node_modules/sass/sass.js" "$@"
else
  exec node  "$basedir/../../../../../node_modules/.pnpm/sass@1.43.3/node_modules/sass/sass.js" "$@"
fi
