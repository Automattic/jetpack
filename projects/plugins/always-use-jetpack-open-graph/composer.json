{
	"name": "automattic/jetpack-always-use-jetpack-open-graph",
	"description": "     ",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"require": {},
	"require-dev": {
		"automattic/jetpack-changelogger": "^3.2"
	},
	"repositories": [
		{
			"type": "path",
			"url": "../../packages/*",
			"options": {
				"monorepo": true
			}
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"autorelease": true,
		"autotagger": true,
		"changelogger": {
			"link-template": "https://github.com/Automattic/jetpack-always-use-jetpack-open-graph/compare/v${old}...v${new}"
		},
		"mirror-repo": "Automattic/jetpack-always-use-jetpack-open-graph",
		"release-branch-prefix": "always-use-jetpack-open-graph",
		"wp-plugin-slug": "always-use-jetpack-open-graph",
		"wp-svn-autopublish": true
	}
}
