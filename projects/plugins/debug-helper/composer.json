{
	"name": "automattic/jetpack-debug-helper",
	"description": "A plugin for mocking various states of the plugin to aid in Jetpack development. Not for production use.",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"require": {},
	"require-dev": {
		"automattic/jetpack-changelogger": "1.0.x-dev"
	},
	"scripts": {
		"pre-update-cmd": [
			"@putenv COMPOSER_ROOT_VERSION=dev-monorepo"
		]
	},
	"repositories": [
		{
			"type": "path",
			"url": "../../packages/*",
			"options": {
				"monorepo": true
			}
		}
	],
	"minimum-stability": "dev",
	"extra": {
		"mirror-repo": "Automattic/jetpack-debug-helper",
		"version-constants": {
			"JETPACK_DEBUG_HELPER_VERSION": "plugin.php"
		},
		"changelogger": {
			"link-template": "https://github.com/Automattic/jetpack-debug-helper/compare/v${old}...v${new}"
		}
	}
}
