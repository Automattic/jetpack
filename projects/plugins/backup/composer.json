{
	"name": "automattic/jetpack-backup-plugin",
	"description": "Easily restore or download a backup of your site from a specific moment in time.",
	"type": "library",
	"license": "GPL-2.0-or-later",
	"require": {
		"automattic/jetpack-autoloader": "^2.10",
		"automattic/jetpack-backup": "^1.1",
		"automattic/jetpack-config": "^1.4",
		"automattic/jetpack-connection": "^1.30",
		"automattic/jetpack-connection-ui": "^1.3",
		"automattic/jetpack-sync": "^1.25",
		"automattic/jetpack-status": "^1.8"
	},
	"require-dev": {
		"yoast/phpunit-polyfills": "0.2.0",
		"automattic/jetpack-changelogger": "^1.2"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"build-development": [
			"Composer\\Config::disableProcessTimeout",
			"pnpm run build"
		],
		"build-production": [
			"Composer\\Config::disableProcessTimeout",
			"pnpm run build-production-concurrently"
		]
	},
	"repositories": [],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"autoloader-suffix": "9559eef123208b7d1b9c15b978567267_backupâ“¥0_2_0"
	},
	"extra": {
		"mirror-repo": "Automattic/jetpack-backup-plugin",
		"release-branch-prefix": "backup",
		"wp-plugin-slug": "jetpack-backup"
	}
}
