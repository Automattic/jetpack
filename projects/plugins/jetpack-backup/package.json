{
	"private": true,
	"description": "Easily restore or download a backup of your site from a specific moment in time.",
	"homepage": "https://jetpack.com",
	"bugs": {
		"url": "https://github.com/Automattic/jetpack/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Automattic/jetpack.git"
	},
	"license": "GPL-2.0-or-later",
	"author": "Automattic",
	"scripts": {
		"build": "yarn install-if-deps-outdated && yarn clean && yarn build-client",
		"build-client": "npx webpack --mode=development",
		"clean": "rm -rf build/",
		"distclean": "rm -rf node_modules && yarn clean",
		"install-if-deps-outdated": "yarn install --check-files --production=false --frozen-lockfile"
	},
	"dependencies": {
		"react": "16.13.1",
		"react-dom": "16.13.1"
	},
	"devDependencies": {
		"@babel/core": "7.13.14",
		"@babel/preset-env": "7.13.12",
		"@babel/preset-react": "7.13.13",
		"@wordpress/browserslist-config": "3.0.1",
		"babel-loader": "8.2.2",
		"webpack": "5.31.0",
		"webpack-cli": "4.6.0"
	},
	"engines": {
		"node": "^14.16.0",
		"yarn": "^1.3.2"
	}
}
