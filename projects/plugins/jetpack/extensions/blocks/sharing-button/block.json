{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "jetpack/sharing-button",
	"title": "Sharing Button",
	"description": "Display a sharing button, allowing users to share your post.",
	"keywords": [],
	"version": "12.5.0",
	"textdomain": "jetpack",
	"category": "embed",
	"parent": [ "jetpack/sharing-buttons" ],
	"attributes": {
		"service": {
			"type": "string"
		},
		"label": {
			"type": "string"
		}
	},
	"usesContext": [ "styleType", "postId", "iconColorValue", "iconBackgroundColorValue" ],
	"supports": {
		"reusable": false,
		"html": false
	},
	"viewScript": "file:./view.js"
}
