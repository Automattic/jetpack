{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "jetpack/ai-paragraph",
	"title": "AI Paragraph (Experimental)",
	"description": "Automatically generate new paragraphs using your existing content, powered by AI magic. We are experimenting with this feature and can tweak or remove it at any point.",
	"keywords": [ "AI", "GPT", "AL", "Magic" ],
	"version": "12.5.0",
	"textdomain": "jetpack",
	"category": "text",
	"icon": "superhero",
	"supports": {
		"align": true,
		"alignWide": true,
		"anchor": true,
		"customClassName": true,
		"className": true,
		"html": true,
		"multiple": true,
		"reusable": false,
		"inserter": false
	},

	"attributes": {
		"content": {
			"type": "string",
			"source": "html"
		},
		"animationDone": {
			"type": "boolean",
			"default": false
		}
	},
	"editorScript": "file:../editor-experimental.js"
}
