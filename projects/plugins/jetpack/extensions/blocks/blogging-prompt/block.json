{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "jetpack/blogging-prompt",
	"title": "Writing Prompt",
	"description": "Answer a new and inspiring writing prompt each day.",
	"keywords": [ "writing", "blogging" ],
	"version": "12.5.0",
	"textdomain": "jetpack",
	"category": "text",
	"supports": {
		"align": false,
		"alignWide": false,
		"anchor": false,
		"className": true,
		"color": {
			"background": true,
			"gradients": true,
			"link": true,
			"text": true
		},
		"customClassName": true,
		"html": false,
		"inserter": true,
		"multiple": false,
		"reusable": true,
		"spacing": {
			"margin": [ "top", "bottom" ],
			"padding": true,
			"blockGap": false
		}
	},
	"editorScript": "file:./editor.js",
	"editorStyle": "file:./editor.css"
}
