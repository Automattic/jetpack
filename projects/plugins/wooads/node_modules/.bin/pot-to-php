#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/javier/projects/Jetpack/node_modules/.pnpm/@wordpress+i18n@4.6.0/node_modules/@wordpress/i18n/tools/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/@wordpress+i18n@4.6.0/node_modules/@wordpress/i18n/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/@wordpress+i18n@4.6.0/node_modules/@wordpress/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/@wordpress+i18n@4.6.0/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/node_modules:/Users/javier/projects/Jetpack/node_modules:/Users/javier/projects/node_modules:/Users/javier/node_modules:/Users/node_modules:/node_modules:/Users/javier/projects/Jetpack/projects/plugins/wooads/node_modules:/Users/javier/projects/Jetpack/projects/plugins/node_modules:/Users/javier/projects/Jetpack/projects/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/javier/projects/Jetpack/node_modules/.pnpm/@wordpress+i18n@4.6.0/node_modules/@wordpress/i18n/tools/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/@wordpress+i18n@4.6.0/node_modules/@wordpress/i18n/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/@wordpress+i18n@4.6.0/node_modules/@wordpress/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/@wordpress+i18n@4.6.0/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/node_modules:/Users/javier/projects/Jetpack/node_modules:/Users/javier/projects/node_modules:/Users/javier/node_modules:/Users/node_modules:/node_modules:/Users/javier/projects/Jetpack/projects/plugins/wooads/node_modules:/Users/javier/projects/Jetpack/projects/plugins/node_modules:/Users/javier/projects/Jetpack/projects/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node_modules/.pnpm/@wordpress+i18n@4.6.0/node_modules/@wordpress/i18n/tools/pot-to-php.js" "$@"
else
  exec node  "$basedir/../../../../../node_modules/.pnpm/@wordpress+i18n@4.6.0/node_modules/@wordpress/i18n/tools/pot-to-php.js" "$@"
fi
