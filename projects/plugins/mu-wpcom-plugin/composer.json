{
	"name": "automattic/jetpack-mu-wpcom-plugin",
	"description": "Test plugin for the jetpack-mu-wpcom package",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"require": {
		"automattic/jetpack-mu-wpcom": "^0.1.0"
	},
	"require-dev": {
		"yoast/phpunit-polyfills": "1.0.4",
		"automattic/jetpack-changelogger": "^3.3.1"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpunit": [
			"./vendor/phpunit/phpunit/phpunit --colors=always"
		],
		"test-coverage": [
			"php -dpcov.directory=. ./vendor/bin/phpunit --coverage-clover \"$COVERAGE_DIR/clover.xml\""
		],
		"test-php": [
			"@composer phpunit"
		]
	},
	"repositories": [],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"autorelease": true,
		"autotagger": true,
		"beta-plugin-slug": "jetpack-mu-wpcom-plugin",
		"mirror-repo": "Automattic/jetpack-mu-wpcom-plugin",
		"release-branch-prefix": "jetpack"
	},
	"config": {
		"autoloader-suffix": "d9d132a783958a00a2c7cccff60ca42d_jetpack_mu_wpcom_pluginâ“¥1_0_0"
	}
}
