{
	"name": "automattic/jetpack-test",
	"description": "test",
	"type": "library",
	"license": "GPL-2.0-or-later",
	"require": {},
	"require-dev": {
		"automattic/jetpack-changelogger": "@dev"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"build-development": [
			"pnpm run build"
		],
		"build-production": [
			"NODE_ENV=production pnpm run build"
		],
		"test-js": [
			"pnpm run test"
		]
	},
	"repositories": [
		{
			"type": "path",
			"url": "../../packages/*",
			"options": {
				"monorepo": true
			}
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true
}
