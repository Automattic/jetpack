{
	"private": true,
	"name": "@automattic/jetpack-forms",
	"version": "0.16.0",
	"description": "Jetpack Forms",
	"homepage": "https://github.com/Automattic/jetpack/tree/HEAD/projects/packages/forms/#readme",
	"bugs": {
		"url": "https://github.com/Automattic/jetpack/labels/[Package] Forms"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Automattic/jetpack.git",
		"directory": "projects/packages/forms"
	},
	"license": "GPL-2.0-or-later",
	"author": "Automattic",
	"scripts": {
		"build": "pnpm run clean && pnpm run build:blocks && pnpm run build:contact-form && pnpm run build:dashboard",
		"build-production": "NODE_ENV=production BABEL_ENV=production pnpm run build && pnpm run validate",
		"build:blocks": "NODE_PATH=\"$PWD/node_modules\" webpack --config ./tools/webpack.config.blocks.js",
		"build:contact-form": "NODE_PATH=\"$PWD/node_modules\" webpack --config ./tools/webpack.config.contact-form.js",
		"build:dashboard": "NODE_PATH=\"$PWD/node_modules\" webpack --config ./tools/webpack.config.dashboard.js",
		"clean": "rm -rf dist/ .cache/",
		"test": "jest",
		"validate": "pnpm exec validate-es --no-error-on-unmatched-pattern dist/",
		"watch": "concurrently 'pnpm:build:blocks --watch' 'pnpm:build:contact-form --watch' 'pnpm:build:dashboard --watch'"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"dependencies": {
		"@automattic/jetpack-components": "workspace:*",
		"@automattic/jetpack-shared-extension-utils": "workspace:*",
		"@wordpress/block-editor": "12.0.0",
		"@wordpress/blocks": "12.9.0",
		"@wordpress/compose": "6.9.0",
		"@wordpress/data": "9.2.0",
		"@wordpress/element": "5.9.0",
		"@wordpress/hooks": "3.32.0",
		"@wordpress/i18n": "4.32.0",
		"@wordpress/icons": "9.23.0",
		"classnames": "2.3.2",
		"email-validator": "2.0.4",
		"gridicons": "3.4.1",
		"lodash": "4.17.21",
		"react-redux": "7.2.8",
		"react-router-dom": "6.10.0",
		"react-transition-group": "^4.4.5",
		"redux": "4.0.5",
		"redux-thunk": "2.3.0",
		"sass": "1.38.1",
		"semver": "7.3.5",
		"webpack": "5.76.0",
		"webpack-cli": "4.9.1"
	},
	"devDependencies": {
		"@automattic/color-studio": "2.5.0",
		"@automattic/jetpack-base-styles": "workspace:*",
		"@automattic/jetpack-webpack-config": "workspace:*",
		"@automattic/remove-asset-webpack-plugin": "workspace:*",
		"@babel/core": "7.21.5",
		"@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
		"@babel/plugin-transform-react-jsx": "7.21.5",
		"@babel/plugin-transform-runtime": "7.21.4",
		"@babel/preset-env": "7.21.5",
		"@babel/runtime": "7.21.5",
		"@wordpress/api-fetch": "6.29.0",
		"@wordpress/babel-plugin-import-jsx-pragma": "4.15.0",
		"@wordpress/browserslist-config": "5.15.0",
		"@wordpress/components": "23.9.0",
		"@wordpress/date": "4.32.0",
		"autoprefixer": "10.4.14",
		"concurrently": "7.6.0",
		"glob": "7.1.6",
		"jest": "29.4.3",
		"jquery": "3.6.0",
		"postcss": "8.4.21",
		"postcss-loader": "6.2.0",
		"sass": "1.38.1",
		"sass-loader": "12.4.0",
		"typescript": "5.0.4"
	},
	"optionalDependencies": {
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"engines": {
		"node": "^18.13.0",
		"yarn": "use pnpm instead - see docs/yarn-upgrade.md"
	}
}
