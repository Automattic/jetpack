{
	"name": "jetpack-post-list",
	"version": "0.1.0-alpha",
	"description": "Enhance the classic view of the Admin section of your WordPress site",
	"main": "src/index.js",
	"repository": "https://github.com/automattic/jetpack-post-list",
	"author": "Automattic",
	"license": "GPL-2.0-or-later",
	"scripts": {
		"install-if-deps-outdated": "pnpm install --no-prod --frozen-lockfile",
		"clean-all": "rm -rf build/ node_modules/ vendor/",
		"build-all": "pnpm run install-if-deps-outdated && pnpm run clean && pnpm run build",
		"clean": "rm -rf build/",
		"build": "wp-scripts build",
		"start": "NODE_ENV=development wp-scripts build",
		"watch": "wp-scripts start",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js"
	},
	"dependencies": {
		"@wordpress/components": "15.0.0",
		"@wordpress/dom-ready": "3.2.1",
		"@wordpress/element": "4.0.0",
		"@wordpress/i18n": "4.2.1"
	},
	"devDependencies": {
		"@babel/runtime": "7.14.0",
		"@wordpress/eslint-plugin": "9.1.1",
		"@wordpress/scripts": "18.0.0"
	},
	"engines": {
		"node": "^16.7.0",
		"pnpm": "^6.5.0",
		"yarn": "use pnpm instead - see docs/yarn-upgrade.md"
	}
}
