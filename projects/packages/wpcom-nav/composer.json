{
	"name": "automattic/jetpack-wpcom-nav",
	"description": "Provides a customized navigation suited for WordPress.com",
	"type": "library",
	"license": "GPL-2.0-or-later",
	"require": {},
	"require-dev": {
		"yoast/phpunit-polyfills": "0.2.0",
		"automattic/jetpack-changelogger": "^1.1"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpunit": [
			"@composer install",
			"./vendor/phpunit/phpunit/phpunit --colors=always"
		],
		"test-coverage": [
			"@composer install",
			"phpdbg -d memory_limit=2048M -d max_execution_time=900 -qrr ./vendor/bin/phpunit --coverage-clover \"$COVERAGE_DIR/clover.xml\""
		],
		"test-php": [
			"@composer phpunit"
		],
		"build-production": "echo 'Add your build step to composer.json, please!'",
		"build-development": "echo 'Add your build step to composer.json, please!'"
	},
	"repositories": [
		{
			"type": "path",
			"url": "../*",
			"options": {
				"monorepo": true
			}
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"mirror-repo": "Automattic/jetpack-wpcom-nav",
		"changelogger": {
			"link-template": "https://github.com/Automattic/jetpack-wpcom-nav/compare/v${old}...v${new}"
		},
		"branch-alias": {
			"dev-master": "0.1.x-dev"
		}
	}
}
