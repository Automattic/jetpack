		"react": "16.14.0",
		"react-dom": "16.14.0",
		"static-site-generator-webpack-plugin": "3.4.2",
		"webpack": "4.46.0"
	},
	"devDependencies": {
		"@wordpress/dependency-extraction-webpack-plugin": "2.9.0",
		"enzyme": "3.11.0",
		"jest": "26.6.3"
	},
	"engines": {
		"node": "^12.20.1",
		"yarn": "^1.3.2"
