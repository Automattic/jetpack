#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/javier/projects/Jetpack/node_modules/.pnpm/webpack-cli@4.9.1_webpack@5.65.0/node_modules/webpack-cli/bin/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/webpack-cli@4.9.1_webpack@5.65.0/node_modules/webpack-cli/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/webpack-cli@4.9.1_webpack@5.65.0/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/node_modules:/Users/javier/projects/Jetpack/node_modules:/Users/javier/projects/node_modules:/Users/javier/node_modules:/Users/node_modules:/node_modules:/Users/javier/projects/Jetpack/projects/packages/connection-ui/node_modules:/Users/javier/projects/Jetpack/projects/packages/node_modules:/Users/javier/projects/Jetpack/projects/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/javier/projects/Jetpack/node_modules/.pnpm/webpack-cli@4.9.1_webpack@5.65.0/node_modules/webpack-cli/bin/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/webpack-cli@4.9.1_webpack@5.65.0/node_modules/webpack-cli/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/webpack-cli@4.9.1_webpack@5.65.0/node_modules:/Users/javier/projects/Jetpack/node_modules/.pnpm/node_modules:/Users/javier/projects/Jetpack/node_modules:/Users/javier/projects/node_modules:/Users/javier/node_modules:/Users/node_modules:/node_modules:/Users/javier/projects/Jetpack/projects/packages/connection-ui/node_modules:/Users/javier/projects/Jetpack/projects/packages/node_modules:/Users/javier/projects/Jetpack/projects/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node_modules/.pnpm/webpack-cli@4.9.1_webpack@5.65.0/node_modules/webpack-cli/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../node_modules/.pnpm/webpack-cli@4.9.1_webpack@5.65.0/node_modules/webpack-cli/bin/cli.js" "$@"
fi
