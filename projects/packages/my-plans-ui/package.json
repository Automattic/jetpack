{
	"private": true,
	"description": "Standalone My Plans page in wp-admin",
	"homepage": "https://jetpack.com",
	"bugs": {
		"url": "https://github.com/Automattic/jetpack/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Automattic/jetpack.git"
	},
	"license": "GPL-2.0-or-later",
	"author": "Automattic",
	"scripts": {
		"build": "pnpm run install-if-deps-outdated && pnpm run clean && pnpm run build-client",
		"build-client": "gulp",
		"clean": "rm -rf build/",
		"install-if-deps-outdated": "pnpm install --no-prod --frozen-lockfile",
		"watch": "pnpm run build && pnpx gulp watch",
		"distclean": "rm -rf node_modules && pnpm run clean",
		"validate-es5": "eslint --parser-options=ecmaVersion:5 --no-eslintrc --no-ignore"
	},
	"dependencies": {
		"@automattic/calypso-build": "6.5.0",
		"@automattic/jetpack-components": "workspace:^0.2.1-alpha",
		"@automattic/jetpack-connection": "workspace:^0.5.1-alpha",
		"@babel/core": "7.12.10",
		"@babel/helper-module-imports": "7.12.5",
		"@babel/preset-env": "7.12.11",
		"@babel/register": "7.12.10",
		"@wordpress/data": "5.2.0",
		"fancy-log": "1.3.3",
		"gulp": "4.0.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-redux": "6.0.1",
		"react-router-dom": "5.2.0",
		"static-site-generator-webpack-plugin": "3.4.2",
		"webpack": "4.46.0"
	},
	"devDependencies": {
		"@babel/runtime": "7.14.0",
		"@wordpress/dependency-extraction-webpack-plugin": "3.2.0",
		"enzyme": "3.11.0",
		"jest": "26.6.3"
	},
	"engines": {
		"node": "^14.16.1",
		"pnpm": "^6.5.0",
		"yarn": "use pnpm instead - see docs/yarn-upgrade.md"
	}
}
