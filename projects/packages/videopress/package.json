{
	"private": true,
	"name": "@automattic/jetpack-videopress",
	"version": "0.2.0-alpha",
	"description": "VideoPress package",
	"homepage": "https://github.com/Automattic/jetpack/tree/HEAD/projects/packages/videopress/#readme",
	"bugs": {
		"url": "https://github.com/Automattic/jetpack/labels/[Package] Videopress"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Automattic/jetpack.git",
		"directory": "projects/packages/videopress"
	},
	"license": "GPL-2.0-or-later",
	"author": "Automattic",
	"scripts": {
		"build": "pnpm run clean && pnpm run build-client",
		"build-client": "NODE_PATH=\"$PWD/node_modules\" webpack",
		"build-concurrently": "pnpm run clean && concurrently 'pnpm:build-client'",
		"build-production-concurrently": "pnpm run clean && concurrently 'NODE_ENV=production BABEL_ENV=production pnpm run build-client' && pnpm run validate",
		"clean": "rm -rf build/",
		"validate": "pnpm exec validate-es build/",
		"watch": "pnpm build && pnpm build-client --watch"
	},
	"devDependencies": {
		"@automattic/calypso-color-schemes": "2.1.1",
		"@automattic/jetpack-base-styles": "workspace:* || ^0.3",
		"@automattic/jetpack-webpack-config": "workspace:* || ^1.3",
		"@wordpress/browserslist-config": "4.1.2",
		"autoprefixer": "10.4.2",
		"postcss": "8.4.14",
		"postcss-custom-properties": "12.1.7",
		"postcss-loader": "6.2.0",
		"sass": "1.54.2",
		"sass-loader": "12.4.0",
		"webpack": "5.72.1",
		"webpack-cli": "4.9.1"
	},
	"engines": {
		"node": "^16.13.2",
		"yarn": "use pnpm instead - see docs/yarn-upgrade.md"
	},
	"dependencies": {
		"@wordpress/icons": "9.3.0",
		"classnames": "2.3.1",
		"concurrently": "7.3.0",
		"debug": "4.3.4",
		"filesize": "8.0.6",
		"tus-js-client": "2.3.0"
	}
}
