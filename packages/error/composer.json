{
	"name": "automattic/jetpack-error",
	"description": "Jetpack Error - a wrapper around WP_Error.",
	"type": "library",
	"license": "GPL-2.0-or-later",
	"require": {},
	"require-dev": {
		"phpunit/phpunit": "^5.7 || ^6.5 || ^7.5"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpunit": [
			"@composer install",
			"./vendor/phpunit/phpunit/phpunit --colors=always"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
