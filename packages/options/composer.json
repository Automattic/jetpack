{
	"name": "automattic/jetpack-options",
	"description": "A wrapper for wp-options to manage specific Jetpack options.",
	"type": "library",
	"license": "GPL-2.0-or-later",
	"require": {
		"automattic/jetpack-constants": "@dev"
	},
	"require-dev": {
		"phpunit/phpunit": "7.*.*",
		"10up/wp_mock": "0.4.2"
	},
	"autoload": {
		"classmap": [
			"legacy"
		]
	},
	"repositories": [
		{
			"type": "path",
			"url": "../*"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true
}
